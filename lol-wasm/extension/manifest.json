{
  "manifest_version": 3,
  "name": "Platypul Lol",
  "description": "Smart bookmarking tool",
  "version": "0.0",
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
  },
  "omnibox": { "keyword" : "p" },
  "background": {
    "service_worker": "platypus.js",
    "type": "module"
  },
  "action": {
    "default_popup": "platypus.html",
    "default_icon": "platypus.png"
  }
}

